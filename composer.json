{
    "name": "ezzaze/binary-search",
    "description": "a PHP package for searching values in large arrays faster",
    "keywords": [
        "ezzaze",
        "binary-search"
    ],
    "homepage": "https://github.com/ezzaze/binary-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Marwane Ezzaze",
            "email": "me@ezzaze.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Ezzaze\\BinarySearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ezzaze\\BinarySearch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
